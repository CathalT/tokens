{
  "checkbox": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows%2BWeb?node-id=4371%3A10197",
    "checked": {
      "#normal": {
        "background": "@theme-control-active-normal",
        "text": "@theme-text-primary-normal",
        "border": "none",
        "icon": "@theme-text-inverted-normal"
      },
      "#hovered": {
        "background": "@theme-control-active-hover",
        "text": "@theme-text-primary-normal",
        "border": "none",
        "icon": "@theme-text-inverted-normal"
      },
      "#pressed": {
        "background": "@theme-control-active-pressed",
        "text": "@theme-text-primary-normal",
        "border": "none",
        "icon": "@theme-text-inverted-normal"
      },
      "#disabled": {
        "background": "@theme-control-active-disabled",
        "text": "@theme-text-primary-disabled",
        "border": "none",
        "icon": "@theme-control-inactive-hover"
      }
    },
    "unchecked": {
      "#normal": {
        "background": "@theme-control-inactive-normal",
        "text": "@theme-text-primary-normal",
        "border": "@theme-outline-input-normal"
      },
      "#hovered": {
        "background": "@theme-control-inactive-hover",
        "text": "@theme-text-primary-normal",
        "border": "@theme-outline-input-normal"
      },
      "#pressed": {
        "background": "@theme-control-inactive-pressed",
        "text": "@theme-text-primary-normal",
        "border": "@theme-outline-input-normal"
      },
      "#disabled": {
        "background": "@theme-control-inactive-hover",
        "text": "@theme-text-primary-disabled",
        "border": "none"
      }
    }
  }
}
