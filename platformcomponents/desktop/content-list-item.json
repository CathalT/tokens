{
  "content-list-item": {
    "comment": "Applied to file attachments from Content tab",
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=49651%3A114181",
    "iconForeground": "@theme-common-text-white",
    "icon": {
      "#default": {
        "background": "@theme-background-accent-normal"
      }
    },
    "#normal": {
      "background": "@theme-background-secondary-normal",
      "textFileName": "@theme-text-primary-normal",
      "textFileSize": "@theme-text-secondary-normal"
    },
    "#hovered": {
      "background": "@theme-background-secondary-hover",
      "textFileName": "@theme-text-primary-normal",
      "textFileSize": "@theme-text-secondary-normal"
    },
    "#disabled": {
      "background": "@theme-background-primary-disabled",
      "textFileName": "@theme-text-primary-disabled",
      "textFileSize": "@theme-text-primary-disabled"
    }
  }
}
